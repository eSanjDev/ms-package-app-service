{
    "name": "esanj/app-service",
    "description": "The **AppService** package is designed to facilitate connections to external services based on a provided client ID.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Esanj Team"
        }
    ],
    "autoload": {
        "psr-4": {
            "Esanj\\AppService\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Esanj\\AppService\\Providers\\AppServiceProvider"
            ]
        }
    },
    "require": {
        "php": "^8.2|^8.3|^8.4",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "esanj/managers": "^0.2.1"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
